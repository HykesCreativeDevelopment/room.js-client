{
  "extends": "airbnb/base",
  "env": {
    "browser": true,
  },
  "rules": {
    "no-param-reassign": [2, {"props": false}],
    "strict": [2, "never"],
    "no-multiple-empty-lines": [2, {"max": 1}],
    "arrow-parens": [2, "as-needed"],
    "no-underscore-dangle": [0],
    "class-methods-use-this": [0]
  }
}
